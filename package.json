{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve:dev": "webpack-dev-server --env --mode development --config config/webpack.base.config.js --open --hot --history-api-fallback --host localhost",
    "start": "npm run serve:dev",
    "serve:dist": "NODE_ENV=production serve dist",
    "clean": "rimraf dist/*",
    "translate": "jsonsort src/translations",
    "build": "npm run clean && webpack --mode production --config config/webpack.prod.config.js --env dist --progress --bail"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix"
    ]
  },
  "engines": {
    "node": "14.15.x",
    "npm": "7.8.x"
  },
  "dependencies": {
    "@ant-design/colors": "^5.0.1",
    "@ant-design/icons": "*",
    "@material-ui/core": "^4.11.3",
    "@reduxjs/toolkit": "*",
    "@sentry/browser": "^5.12.5",
    "antd": "4.5.1",
    "antd-mask-input": "^0.1.14",
    "autosuggest-highlight": "^3.1.1",
    "bootstrap": "^4.5.2",
    "clipboard-copy": "^3.1.0",
    "connected-react-router": "^6.6.1",
    "css-loader": "^3.2.0",
    "history": "4.10.1",
    "invariant": "^2.2.4",
    "jquery": "^3.4.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "memorystorage": "^0.11.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "query-string": "^6.10.1",
    "react": "^16.8.6",
    "react-currency-input": "^1.3.6",
    "react-dom": "^16.8.6",
    "react-file-drop": "^0.2.8",
    "react-number-format": "^4.4.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-slick": "^0.25.2",
    "react-swipeable-views": "^0.13.3",
    "reactstrap": "^8.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.0.5",
    "request": "^2.88.0",
